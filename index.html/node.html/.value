<div class="Node">
  <style>
  .Node .content{ padding-left: 10px; }
  </style>

  <div class="title"></div>
  <div class="content"></div>

  <script>
  (async ()=>{
    const component = document.currentScript.parentElement;
    const args = component.var.args;
    component.querySelector(".title").innerText = args.key;
  })();
  </script>
</div>
